        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/dialogo_confirmacion_pedido_5_pagoTotal"
        app:layout_constraintEnd_toStartOf="@+id/dialogo_confirmacion_pedido_5_pagoTotal"
