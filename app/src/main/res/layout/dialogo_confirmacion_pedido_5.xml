        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/dialogo_confirmacion_pedido_5_pagoTotal"
        app:layout_constraintTop_toTopOf="@+id/dialogo_confirmacion_pedido_5_pagoTotal" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$"
        app:layout_constraintBottom_toBottomOf="@+id/cantidadFinanciada"
        app:layout_constraintEnd_toStartOf="@+id/cantidadFinanciada"
